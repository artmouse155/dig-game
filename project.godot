config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="Dig Game"
run/main_scene="res://Scenes/mainScene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://Assets/Textures/logo.png"

[autoload]

Game="*res://Scripts/Autoload/Game.gd"
Statistics="*res://Scripts/Autoload/Statistics.gd"
Debug="*res://Scripts/Autoload/Debug.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_conventions_cheat_sheet/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

[file_customization]

folder_colors={
"res://Animated Button/": "pink",
"res://Assets/": "red",
"res://Resources/": "yellow",
"res://Scenes/": "blue",
"res://Scripts/": "green",
"res://addons/": "gray"
}

